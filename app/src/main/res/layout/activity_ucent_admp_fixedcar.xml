<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/background">

    <include layout="@layout/common_toolbar"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_64"
        android:background="@color/background"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/px_48"
        android:text="@string/park_info"
        android:textColor="@color/dark_gray"
        android:textSize="@dimen/px_24" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_88"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/px_48"
            android:textColor="@color/black"
            android:textSize="@dimen/px_32"
            android:text="@string/park_platenumber"/>
        <TextView
            android:id="@+id/tc_ucent_admp_platenumber"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/px_48"
            android:gravity="end"
            android:text="川A66666  川A66667"
            android:textColor="@color/light_black"
            android:textSize="@dimen/px_30" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_88"
        android:layout_marginTop="1dp"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/px_48"
            android:text="@string/park_type"
            android:textColor="@color/black"
            android:textSize="@dimen/px_32" />

        <TextView
            android:id="@+id/tc_ucent_admp_parktype"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/px_48"
            android:gravity="end"
            android:text="固定车位 A-103"
            android:textColor="@color/light_black"
            android:textSize="@dimen/px_30" />

</LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_88"
        android:layout_marginTop="1dp"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/px_48"
            android:textColor="@color/black"
            android:textSize="@dimen/px_32"
            android:text="@string/park_location"/>

        <TextView
            android:id="@+id/tc_ucent_admp_location"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/px_48"
            android:gravity="end"
            android:text="园区A栋-2F东南角"
            android:textColor="@color/light_black"
            android:textSize="@dimen/px_30" />

    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_64"
        android:background="@color/background"
        android:textColor="@color/dark_gray"
        android:textSize="@dimen/px_24"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/px_48"
        android:text="@string/park_apply_duration"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_88"
        android:layout_marginTop="1dp"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/px_48"
            android:textColor="@color/black"
            android:textSize="@dimen/px_32"
            android:text="@string/park_apply_duration"/>

        <TextView
            android:id="@+id/tc_ucent_admp_applyduration"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/px_48"
            android:gravity="end"
            android:text="12月"
            android:textColor="@color/light_black"
            android:textSize="@dimen/px_30" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_88"
        android:layout_marginTop="1dp"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/px_48"
            android:textColor="@color/black"
            android:textSize="@dimen/px_32"
            android:text="@string/park_use_deadline"/>
        <TextView
            android:id="@+id/tc_ucent_admp_deadline"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/px_48"
            android:gravity="end"
            android:text="2017年6月-2018年6月"
            android:textColor="@color/light_black"
            android:textSize="@dimen/px_30" />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_64"
        android:background="@color/background"
        android:textColor="@color/dark_gray"
        android:textSize="@dimen/px_24"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/px_48"
        android:text="@string/business_pending_authority"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_88"
        android:background="@color/white"
        android:paddingLeft="@dimen/px_48"
        android:textColor="@color/black"
        android:text="无"
        android:gravity="center_vertical"
        android:textSize="@dimen/px_30"/>




</LinearLayout>
